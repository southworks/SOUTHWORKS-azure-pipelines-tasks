{
    "manifestVersion": 1,
    "id": "binaries-comparer",
    "name": "Binaries Comparer",
    "version": "0.2.1",
    "publisher": "SOUTHWORKS",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Compares binaries to test incompatibilities",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "content": {
        "details": {
            "path": "readme.md"
        }
    },
    "links": {
        "repository": {
            "uri": "https://github.com/southworkscom/SOUTHWORKS-azure-pipelines-tasks/tree/master/Tasks/ApiCompat"
        },
        "issues": {
            "uri": "https://github.com/southworkscom/SOUTHWORKS-azure-pipelines-tasks/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/southworkscom/SOUTHWORKS-azure-pipelines-tasks/tree/master/Tasks/ApiCompat"
    },
    "files": [
		{
			"path": "source/lib/"
		},
		{
			"path": "source/ApiCompat/"
		},
		{
			"path": "source/node_modules/"
		},
		{
			"path": "source/.eslintrc.js"
		},
		{
			"path": "source/icon.png"
		},
		{
			"path": "source/package.json"
		},
		{
			"path": "source/task.json"
		},
		{
			"path": "source/tsconfig.json"
		}
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "source"
            }
        }
    ],
    "galleryFlags": [
        "Public"
    ]
}
